	private static String getChCat(int chNum) {
		
		if(chNum <=  180 )
			return "OTHERS";
		if(chNum <=  256 )
			return "MOVIES_ENGLISH";
		if(chNum <=  180 )
			return "";
		if(chNum <=  180 )
			return "";
		if(chNum <=  180 )
			return "";
		if(chNum <=  180 )
			return "";
		if(chNum <=  180 )
			return "";
		if(chNum <=  180 )
			return "";
		if(chNum <=  180 )
			return "";
		
		
		
		return null;
	}

