	private String  channelDisplayName;
	private int channelNum;
	private Integer chImgResourceId;
	private V2ChannelCategoryEnum categories[];
	
	ChannelDetailEnum(String name,int num,Integer chImgResourceID, V2ChannelCategoryEnum... categories){
		this.channelDisplayName = name;
		this.channelNum = num;
		this.chImgResourceId = chImgResourceID;
		if(categories != null)
			this.categories = categories;
	}
	
	String displayName(){
		return channelDisplayName;
	}
	
	int channelNumber(){
		return channelNum;
	}
	
	public V2ChannelCategoryEnum[] getChannelCategories(){
			return categories;
	}
	
}
