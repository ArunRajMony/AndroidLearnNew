	private String  channelDisplayName;
	private int channelNum;
	private V2ChannelCategoryEnum categories[];
	
	ChannelDetailEnum(String name,int num,V2ChannelCategoryEnum... categories){
		this.channelDisplayName = name;
		this.channelNum = num;
		if(categories != null)
			this.categories = categories;
	}
	
	String displayName(){
		return channelDisplayName;
	}
	
	int channelNumber(){
		return channelNum;
	}
	
	public V2ChannelCategoryEnum[] channelCategories(){
			return categories;
	}
	
}
